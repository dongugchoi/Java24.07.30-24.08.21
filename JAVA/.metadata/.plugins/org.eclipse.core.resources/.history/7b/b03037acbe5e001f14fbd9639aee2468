package ex02_Enum;

public enum Operation {
	PLUS{
		Double plus(double x , double y) {
			return x+y;
		}
	},
	
	MINUS{
		Double minus(double x, double y) {
			return x-y;
		}
	},
	
	MULTI{
		Double multi(double x, double y) {
			return x*y;
		}
	},
	
	DIVDI{
		Double divdi(Double x, Double y) {
			return x/y;
		}
	}
	
}
